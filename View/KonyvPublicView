class KonyvPublicView{
    #elem;
    constructor(elem, szuloElem){
        this.#elem = elem;
        szuloElem.append(`
            <div>
                <h2>${elem.cim}</h2>
                <p>${elem.szerzo}</p>
                <span>${elem.ar}</span>
                <button>Kos√°rba</button>
            </div>
        `);
        this.konyvPublicElem = szuloElem.children("div:last-child");

    }
}

export default KonyvPublicView;